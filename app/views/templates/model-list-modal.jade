div.modal.hide.fade(id='{{modalId}}', tabindex='-1', role='dialog')

    div.modal-header
        button.close(data-dismiss='modal') x
        h3 {{type}}

    div.modal-body
        div(cr-model-list, type='{{type}}')

    div.modal-footer.btn-toolbar
        button.btn.pull-right(ng-click='cancel', data-dismiss='modal') Cancel